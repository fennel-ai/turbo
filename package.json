{
  "name": "turbo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "storybook"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test --parallel",
    "coverage": "turbo run coverage --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-fennel": "workspace:*",
    "prettier": "^2.8.8",
    "turbo": "^1.10.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.21.0"
}